{
  "license": "MIT",
  "name": "tdd-in-javascript",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": "8.10.0",
    "npm": "5.6.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run spec"
    }
  },
  "scripts": {
    "coverage": "cross-env NODE_ENV=TEST nyc --reporter=html --reporter=text --reporter=text-summary npm run spec",
    "coverage:ci": "npm run coverage -s",
    "prepush": "npm run coverage:ci",
    "spec": "mochapack --opts config/mocha/mochapack.opts",
    "spec:ci": "npm run spec -s",
    "spec:watch": "npm run spec -- --watch",
    "tdd": "npm run spec:watch"
  },
  "dependencies": {
    "fs-promise": "2.0.3",

    "immutable": "4.0.0-rc.12",
    "immutable-ext": "1.1.5",
    "ramda": "0.26.1",
    "ramda-adjunct": "2.17.0",
    "timekeeper": "2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/register": "7.4.4",
    "@istanbuljs/nyc-config-babel": "2.1.1",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.6",
    "babel-plugin-istanbul": "5.1.4",
    "bluebird": "3.5.4",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "diff-so-fancy": "1.2.5",
    "eslint": "5.16.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-react": "7.13.0",
    "growl": "1.10.5",
    "husky": "2.3.0",
    "mocha": "6.1.4",
    "mocha-loader": "2.0.1",
    "mochapack": "1.1.1",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "should": "13.2.3",
    "source-map-support": "0.5.12",
    "testdouble": "3.11.0",
    "webpack": "4.31.0",
    "webpack-node-externals": "1.7.2"
  }
}
